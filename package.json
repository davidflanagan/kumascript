{
    "name": "kumascript",
    "description": "Scripting for MDN",
    "version": "0.2.0",
    "license": "MPL-2.0",
    "homepage": "https://github.com/mdn/kumascript",
    "bugs": "https://github.com/mdn/kumascript/issues",
    "author": "Les Orchard <lorchard@mozilla.com> (http://lmorchard.com/) and David Flanagan <dflanagan@mozilla.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/mdn/kumascript.git"
    },
    "main": "./index",
    "scripts": {
        "start": "node index.js",
        "test": "jest",
        "lint": "eslint *.js src tests",
        "lint-json": "jsonlint -q *.json `find src macros tests -name '*.json'`",
        "format": "./node_modules/.bin/prettier --write --require-pragma *.js src/*.js tests/*.js tests/macros/*.js"
    },
    "engines": {
        "node": "10.14"
    },
    "dependencies": {
        "ejs": "2.6.1",
        "express": "4.16",
        "lru-cache": "5.1.1",
        "mdn-browser-compat-data": "0.0.59",
        "morgan": "1.9.1",
        "newrelic": "4.13.0",
        "request": "2.88.0",
        "winston": "3.1.0"
    },
    "devDependencies": {
        "eslint": "^5.12.0",
        "eslint-config-prettier": "^3.3.0",
        "eslint-plugin-jest": "^22.1.2",
        "eslint-plugin-prettier": "^3.0.1",
        "jest": "^23.6.0",
        "jsonlint": "^1.6.3",
        "pegjs": "^0.10.0",
        "prettier": "^1.15.3"
    }
}
